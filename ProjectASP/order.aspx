<!-- Grand Total -->
<h3>Total Bill: ₹<span id="grandTotal">0</span></h3>

<!-- CSS (Styling) -->
<style>
    body {
        font-family: Arial, sans-serif;
    }

    h2, h3 {
        color: #333;
    }

    .menu-item {
        border: 1px solid #ccc;
        padding: 10px;
        margin: 10px;
        width: 200px;
        text-align: center;
        background-color: #f9f9f9;
        border-radius: 8px;
        box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.1);
    }

    .food-image {
        width: 100px;
        height: 100px;
        border-radius: 5px;
    }

    .quantity-controls {
        display: flex;
        justify-content: center;
        align-items: center;
        margin: 5px 0;
    }

    .btn-increase, .btn-decrease {
        background-color: #007bff;
        color: white;
        border: none;
        width: 30px;
        height: 30px;
        font-size: 18px;
        cursor: pointer;
        margin: 0 5px;
        border-radius: 5px;
    }

    .btn-decrease {
        background-color: #dc3545;
    }

    .quantity {
        text-align: center;
        width: 40px;
        font-size: 16px;
        border: 1px solid #ccc;
        border-radius: 5px;
        background-color: white;
    }

    #grandTotal {
        font-size: 24px;
        color: #007bff;
        font-weight: bold;
    }
</style>

<!-- JavaScript for Quantity Buttons -->
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>
    $(document).ready(function () {
        function updateGrandTotal() {
            var totalBill = 0;
            $(".menu-item").each(function () {
                var total = parseFloat($(this).find(".total").text().replace("₹", ""));
                totalBill += total;
            });
            $("#grandTotal").text(totalBill.toFixed(2));
        }

        $(".btn-increase").click(function () {
            var qtyInput = $(this).siblings(".quantity");
            var price = parseFloat($(this).closest(".menu-item").find(".price").text());
            var totalSpan = $(this).closest(".menu-item").find(".total");

            var qty = parseInt(qtyInput.val());
            qty++;
            qtyInput.val(qty);

            var newTotal = qty * price;
            totalSpan.text("₹" + newTotal.toFixed(2));

            updateGrandTotal();
        });

        $(".btn-decrease").click(function () {
            var qtyInput = $(this).siblings(".quantity");
            var price = parseFloat($(this).closest(".menu-item").find(".price").text());
            var totalSpan = $(this).closest(".menu-item").find(".total");

            var qty = parseInt(qtyInput.val());
            if (qty > 1) {
                qty--;
                qtyInput.val(qty);

                var newTotal = qty * price;
                totalSpan.text("₹" + newTotal.toFixed(2));

                updateGrandTotal();
            }
        });

        updateGrandTotal();
    });
</script>


